msgstr ""

#: ../../technical/customise/app/settings.rst:62
msgid "``MENU_ALLOW_SYNC`` (boolean): show the \"Sync\" option in the main menu"
msgstr ""

#: ../../technical/customise/app/settings.rst:63
msgid ""
"``MENU_ALLOW_LOGOUT`` (boolean): show the \"Logout\" option in the main "
"menu"
msgstr ""

#: ../../technical/customise/app/settings.rst:64
msgid ""
"``MENU_ALLOW_DOWNLOAD`` (boolean): show the \"Download\" option in the "
"main menu"
msgstr ""

#: ../../technical/customise/app/settings.rst:65
msgid ""
"``MENU_ALLOW_LANGUAGE`` (boolean): show the \"Language\" option in the "
